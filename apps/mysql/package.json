{
  "version": "1.0.0",
  "name": "mysql-hs",
  "app_hyperproof": {
    "name": "MySQL Server",
    "appType": "hypersync",
    "authType": "custom",
    "category": "Database Servers",
    "descriptionCapabilities": "This integration allows users to create Hypersyncs to MySQL servers.",
    "debug": true,
    "staticIp": true
  },
  "scripts": {
    "build": "npm run lint && tsc && npm run copydecl",
    "up": "node ./build/start.js",
    "copydecl": "rimraf ./build/json && copyfiles -u 2 ./src/json/*.json ./src/json/proof/*.json ./build/json",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*.ts"
  },
  "engines": {
    "node": "^16.19.1 || ^18.17.1"
  },
  "dependencies": {
    "@hyperproof/hypersync-sdk": "^3.1.0",
    "http-errors": "1.8.0",
    "http-status-codes": "2.1.4",
    "jsonwebtoken": "8.5.1",
    "mysql": "2.18.1",
    "node-fetch": "2.6.7",
    "superagent": "7.1.5"
  },
  "devDependencies": {
    "@types/http-errors": "^1.8.2",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.18.48",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-node": "8.0.2",
    "typescript": "4.4.4"
  }
}
